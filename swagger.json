  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "username": {
            "type": "string",
            "description": "Required. 150 characters or fewer. Letters, digits and @/./+/-/_ only.",
            "pattern": "^[\\w.@+-]+\\z",
            "maxLength": 150
          },
          "password": {
            "type": "string",
            "writeOnly": true,
            "maxLength": 128
          },
          "email": {
            "type": "string",
            "format": "email",
            "maxLength": 254
          },
          "phone_number": {
            "type": "string",
            "nullable": true,
            "maxLength": 15
          },
          "role": {
            "enum": [
              "admin",
              "customer",
              "business_owner"
            ],
            "type": "string"
          }
        },
        "required": [
          "username",
          "password",
          "phone_number"
        ]
      },
      "Business": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "owner": {
            "type": "integer"
          },
          "business_name": {
            "type": "string",
            "maxLength": 200
          },
          "business_email": {
            "type": "string",
            "format": "email",
            "maxLength": 254
          },
          "business_phone": {
            "type": "string",
            "maxLength": 15
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          }
        },
        "required": [
          "owner",
          "business_name",
          "business_phone"
        ]
      },
      "Transaction": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "sender": {
            "type": "integer"
          },
          "receiver": {
            "type": "integer"
          },
          "amount": {
            "type": "string",
            "format": "decimal",
            "multipleOf": 0.01,
            "maximum": 100000000,
            "minimum": -100000000
          },
          "transaction_type": {
            "enum": [
              "credit",
              "debit"
            ],
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          },
          "status": {
            "enum": [
              "pending",
              "completed",
              "failed"
            ],
            "type": "string"
          }
        },
        "required": [
          "sender",
          "receiver",
          "amount",
          "transaction_type",
          "status"
        ]
      },
      "Notification": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "user": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          },
          "is_read": {
            "type": "boolean"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          }
        },
        "required": [
          "user",
          "message"
        ]
      },
      "AuthToken": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "writeOnly": true
          },
          "password": {
            "type": "string",
            "writeOnly": true
          },
          "token": {
            "type": "string",
            "readOnly": true
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "TokenObtainPair": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "writeOnly": true
          },
          "password": {
            "type": "string",
            "writeOnly": true
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "TokenRefresh": {
        "type": "object",
        "properties": {
          "refresh": {
            "type": "string"
          },
          "access": {
            "type": "string",
            "readOnly": true
          }
        },
        "required": [
          "refresh"
        ]
      }
    }
  }
